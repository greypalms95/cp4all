        "# while (print 1 to n)\n",
        "i = 1 # initialise\n",
        "n = 10\n",
        "while i <= n: # loop condition\n",
        "  print(i, end=' ') # loop body\n",
        "  i = i + 1 # update loop control\n",
        "  # i += 1 "
      ],
      "execution_count": null,
      "outputs": [
